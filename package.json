{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "react-scripts": "1.1.4",
    "tailwindcss": "^0.6.1"
  },
  "scripts": {
    "build": "npm run tailwind:css && GENERATE_SOURCEMAP=false react-scripts build",
    "eject": "react-scripts eject",
    "start": "npm run tailwind:css && PORT=3000 react-scripts start",
    "tailwind:css": "tailwind build src/main.css -o src/index.css",
    "test": "react-scripts test --env=jsdom"
