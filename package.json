{
  "name": "create-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "react-redux": "~5.0.6",
    "react-router-dom": "~4.2.2",
    "redux": "~3.7.2",
    "redux-thunk": "~2.2.0"
  },
  "devDependencies": {
    "bootstrap": "~4.0.0-beta.2",
    "node-sass-chokidar": "~0.0.3",
    "npm-run-all": "~4.1.2",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "build": "npm run build:css && GENERATE_SOURCEMAP=false react-scripts build",
    "build:css": "node-sass-chokidar ./src/assets/styles/app.scss -o ./src/assets/styles",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch:css start:js",
    "start:js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch:css": "npm run build:css && node-sass-chokidar ./src/assets/styles/app.scss -o ./src/assets/styles --watch --recursive"
  }
}
