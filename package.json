{
  "name": "create-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.11",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "bootstrap": "^4.0.0-beta",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2"
  },
  "scripts": {
    "build": "npm run build-css && react-scripts build && npm run build-server-templates",
    "build-css": "node-sass-chokidar ./src/assets/styles/app.scss -o ./src/assets/styles",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/assets/styles/app.scss -o ./src/assets/styles --watch --recursive"
  }
}
